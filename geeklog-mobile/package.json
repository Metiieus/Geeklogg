{
  "name": "geeklog-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web --output-dir dist",
    "build:capacitor": "node build-capacitor.js",
    "cap:sync": "npx cap sync",
    "cap:android": "npx cap open android",
    "cap:ios": "npx cap open ios",
    "cap:run:android": "npx cap run android",
    "cap:run:ios": "npx cap run ios",
    "cap:livereload:android": "npx cap run android --livereload",
    "cap:livereload:ios": "npx cap run ios --livereload",
    "cap:build:android": "npm run build:web && npx cap sync android && npx cap open android",
    "cap:build:ios": "npm run build:web && npx cap sync ios && npx cap open ios"
  },
  "dependencies": {
    "@capacitor/core": "^7.4.2",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "expo": "~53.0.17",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-notifications": "^0.31.4",
    "expo-status-bar": "~2.2.3",
    "firebase": "^11.10.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.11.1",
    "react-native-vector-icons": "^10.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@capacitor/android": "^7.4.2",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3",
    "vite-plugin-pwa": "^1.0.1"
  },
  "private": true
}
